{
  "autoload": {
    "psr-4": {
      "Capture\\": "src/",
      "Capture\\Config\\": "config/",
      "Exception\\": "src/Exception"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Capture\\": "tests/",
      "Capture\\Config\\": "config/"
    }
  },
  "extra": {
    "aura": {
      "type": "project",
      "config": {
        "common": "Capture\\Config\\Common"
      }
    }
  },
  "require": {
    "php": ">=5.6.30",
    "aura/cli-kernel": "~2.0",
    "symfony/process": "^3.4",
    "vlucas/phpdotenv": "^2.0",
    "guzzlehttp/guzzle": "^6.0",
    "monolog/monolog": "@stable"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
